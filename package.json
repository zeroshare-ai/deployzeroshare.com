{
  "name": "deployzeroshare",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:smoke": "playwright test tests/smoke.spec.ts",
    "test:links": "playwright test tests/links.spec.ts",
    "test:form": "playwright test tests/support-form.spec.ts",
    "test:forms": "playwright test tests/forms.spec.ts",
    "test:api": "playwright test tests/api-health.spec.ts",
    "test:pre-deploy": "playwright test tests/pre-deployment.spec.ts",
    "test:screenshots": "playwright test tests/screenshots.spec.ts",
    "test:deployed": "BASE_URL=https://deployzeroshare.com playwright test tests/deployment.spec.ts",
    "test:all": "playwright test",
    "test:qa": "npm run test:pre-deploy && npm run test:api && npm run test:forms && npm run test:smoke",
    "test:install": "playwright install --with-deps",
    "qa": "npm run build && npm run test:qa",
    "images:list": "node scripts/generate-images.js --list",
    "images:all": "node scripts/generate-images.js",
    "images:priority1": "node scripts/generate-images.js --priority 1",
    "images:priority2": "node scripts/generate-images.js --priority 2",
    "images:priority3": "node scripts/generate-images.js --priority 3",
    "images:priority4": "node scripts/generate-images.js --priority 4",
    "images:blogs": "node scripts/generate-images.js --priority 2 && node scripts/generate-images.js --priority 4",
    "compliance:generate": "node scripts/compliance/generate-evidence.js",
    "compliance:soc2": "node scripts/compliance/generate-evidence.js --cert=soc2",
    "compliance:iso27001": "node scripts/compliance/generate-evidence.js --cert=iso27001",
    "compliance:gdpr": "node scripts/compliance/generate-evidence.js --cert=gdpr",
    "compliance:hipaa": "node scripts/compliance/generate-evidence.js --cert=hipaa",
    "compliance:pci": "node scripts/compliance/generate-evidence.js --cert=pci",
    "compliance:ccpa": "node scripts/compliance/generate-evidence.js --cert=ccpa",
    "compliance:auditor-package": "node scripts/compliance/generate-evidence.js --package"
  },
  "dependencies": {
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@google/genai": "^1.38.0",
    "@playwright/test": "^1.57.0",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "playwright": "^1.57.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "typescript": "^5.3.3"
  }
}
